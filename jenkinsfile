pipeline{
   agent any
    stages{
        stage('GetCode'){
            steps{
                git 'https://github.com/kgovinda1995/jenkins.git'
            }
         } 
         agent{
            label 'node-1'
         }
        stage ('Maven build') {
            steps {
            sh 'mvn clean install'
            }
            
        }       
    }
}
